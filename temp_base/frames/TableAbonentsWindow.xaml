<Window x:Class="temp_base.frames.TableAbonentsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:temp_base.frames"
        mc:Ignorable="d"
        Title="TableAbonentsWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="TableAbonents" RenderTransformOrigin="0.488,0.669" Margin="0,-10,0,10">
            <DataGrid.Columns>
                <DataGridTextColumn x:Name="column_id" Header="ID"/>
                <DataGridTextColumn x:Name="column_date" Header="Дата последней тразакции"/>
                <DataGridTextColumn x:Name="column_fio" Header="ФИО"/>
                <DataGridTextColumn x:Name="column_money" Header="Сумма счета"/>
                <DataGridTextColumn x:Name="column_rep" Header="Номер РЭП"/>
                <DataGridTextColumn x:Name="column_amouth" Header="Сумма долга"/>
                <DataGridTextColumn x:Name="column_adress" Header="Адрес"/>
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Window>
